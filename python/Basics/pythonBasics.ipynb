   ],
   "source": [
    "x = 5\n",
    "y = 2\n",
    "print(x + y) # prints 7\n",
    "print(x - y) # prints 3\n",
    "print(x * y) # prints 10\n",
    "print(x / y) # prints 2.5\n",
    "print(x % y) # prints 1\n",
    "print(x ** y) # prints 25\n",
    "print(x // y) # prints 2"
   ],
   "metadata": {
